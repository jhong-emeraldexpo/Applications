    <validationRules>
        <fullName>First_Name_Email_or_Phone_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(ISBLANK(Phone), 
ISBLANK(Email)), 
ISBLANK(FirstName))&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorMessage>First Name, Email/Phone fields are required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>State_required_for_specific_Countries</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   AND($User.Email &lt;&gt; 'salesforce@emeraldexpo.com',
         SkipAddressValidation__c = FALSE)
   ,
OR(
ISPICKVAL(MailingCountryCode,'US'),
ISPICKVAL(MailingCountryCode,'CA')
),
(NOT ISBLANK(MailingStreet)),
(NOT ISBLANK(MailingCity)),
ISBLANK(TEXT(MailingStateCode))
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>MailingState</errorDisplayField>
        <errorMessage>State/Province is required for US or Canadian address</errorMessage>
    </validationRules>
