    <validationRules>
        <fullName>CancelledOrLostRequiredWhenObjectionCode</fullName>
        <active>true</active>
        <description>Cannot select an Objection Code, unless an Opportunity Stage of Lost or Cancelled is selected</description>
        <errorConditionFormula>AND(
AND(
   !INCLUDES( Opportunity_Stage__c,'Lost'),!INCLUDES(Opportunity_Stage__c,'Cancelled')),(!ISBLANK( Objection_Code__c )))</errorConditionFormula>
        <errorDisplayField>Objection_Code__c</errorDisplayField>
        <errorMessage>Cannot select an Objection Code unless an Opportunity Stage of Lost or Cancelled is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EventRequired</fullName>
        <active>true</active>
        <description>Event is required if this is a dynamic segment</description>
        <errorConditionFormula>AND ( (IsDynamic__c = True),ISBLANK(Event__c))</errorConditionFormula>
        <errorDisplayField>Event__c</errorDisplayField>
        <errorMessage>Please select the current event for the show linked to this segment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityStageNotRequiredForProspect</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 Has_No_Opportunity__c = TRUE, NOT(ISBLANK(  Opportunity_Stage__c  )))</errorConditionFormula>
        <errorDisplayField>Opportunity_Stage__c</errorDisplayField>
        <errorMessage>Opportunity Stage is not required for Prospect List.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpptyStageRequiredFor</fullName>
        <active>true</active>
        <description>If Dynamic Maintained = True AND  ISProspect List = FALSE
Required Fields

•	Opportunity Stage</description>
        <errorConditionFormula>AND(IsDynamic__c == TRUE, Has_No_Opportunity__c ==FALSE,  ISBLANK( Opportunity_Stage__c ))</errorConditionFormula>
        <errorDisplayField>Opportunity_Stage__c</errorDisplayField>
        <errorMessage>Please Enter an Opportunity Stage for Dynamic Lists for non-Prospect Lists</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PardotListRequired</fullName>
        <active>true</active>
        <description>Pardot List Name is required if Sync with Pardot List is checked.</description>
        <errorConditionFormula>AND(IsSyncWithPardot__c  = True, ISBlank(Pardot_List__c ))</errorConditionFormula>
        <errorDisplayField>Pardot_List__c</errorDisplayField>
        <errorMessage>Pardot List is required when Sync with Pardot List is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SelectAccountUniverse</fullName>
        <active>true</active>
        <description>Select account universe if checkbox is this is prospect list = TRUE</description>
        <errorConditionFormula>AND
(
  Has_No_Opportunity__c==TRUE, 
  AND
     (
       !ISPICKVAL(
         Account_Universe_Method__c, "Lead Scoring")
         
            , 
       !ISPICKVAL(
           Account_Universe_Method__c, "Account Segment")
           )
)</errorConditionFormula>
        <errorDisplayField>Account_Universe_Method__c</errorDisplayField>
        <errorMessage>Account Universe Method is required for Prospect Lists</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubExpoRequiredForDynamicNonProspects</fullName>
        <active>true</active>
        <description>If Dynamic Maintained = True AND  ISProspect List = FALSE
Required Fields


IF Sub Show IS NOT BLANK
•	SubExpo</description>
        <errorConditionFormula>AND(IsDynamic__c == TRUE, Has_No_Opportunity__c ==FALSE,  !ISBLANK( SubShow__c ), ISBLANK(SubExpo__c))</errorConditionFormula>
        <errorDisplayField>SubExpo__c</errorDisplayField>
        <errorMessage>Please enter a Subexpo to define Opportunity Exclusion for Dynamic, non-Prospect List.</errorMessage>
    </validationRules>
