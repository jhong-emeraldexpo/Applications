        <formula>Case(0, 
IF(Target_Start_Date__c &gt; Today(), VALUE('Null') ,0), 
IF( (Target_End_Date__c &gt;= TODAY() &amp;&amp; Target_Start_Date__c &lt;=Today()) ,(Target_End_Date__c - TODAY() )/7, 0), 
IF( Target_End_Date__c &lt;Today(), 0, 0) 
)</formula>
