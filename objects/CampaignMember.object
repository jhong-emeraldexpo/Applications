    <validationRules>
        <fullName>Lost_Detailed_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
IsPickval( Objection_Code__c, 'Other'),
IsPickval( Objection_Code__c, 'Prefers another Show (Indicate which show)')),
(ISBLANK(Lost_Details__c)))
&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Lost_Details__c</errorDisplayField>
        <errorMessage>Please provide additional details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ObjectionCodeRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IsPickval( Status , 'Not Interested'),
(ISBLANK(Text( Objection_Code__c ))))
&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Objection_Code__c</errorDisplayField>
        <errorMessage>Objection Code is required.</errorMessage>
    </validationRules>
