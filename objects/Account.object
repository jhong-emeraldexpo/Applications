    <validationRules>
        <fullName>Alert_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
INCLUDES(Do_Not_Sell__c, "Collections Alert"),
ISBLANK (TEXT(Collection_Alert_Reason__c))
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Collection_Alert_Reason__c</errorDisplayField>
        <errorMessage>Collection Alert Reason is required when Collections Alert is chosen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Postal_Code_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
   AND($User.Email &lt;&gt; 'salesforce@emeraldexpo.com',
         SkipAddressValidation__c = FALSE)
   ,
   OR( 
       ISPICKVAL(BillingCountryCode,'US'), 
       ISPICKVAL(BillingCountryCode,'CA') 
   ),
   (NOT ISBLANK(BillingStreet)),
   (NOT ISBLANK(BillingCity)),
   ISBLANK(BillingPostalCode)  
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>BillingPostalCode</errorDisplayField>
        <errorMessage>Postal Code is required for US or Canadian address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>State_required_for_specific_Countries</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   AND($User.Email &lt;&gt; 'salesforce@emeraldexpo.com',
         SkipAddressValidation__c = FALSE)
   ,
OR(
ISPICKVAL(BillingCountryCode,'US'),
ISPICKVAL(BillingCountryCode,'CA')
),
(NOT ISBLANK(BillingStreet)),
(NOT ISBLANK(BillingCity)),
ISBLANK(BillingState)
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>BillingState</errorDisplayField>
        <errorMessage>State/Province is required for US or Canadian address.</errorMessage>
    </validationRules>
