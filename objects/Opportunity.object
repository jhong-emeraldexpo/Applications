    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>A2Z_Required_Field</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
Probability &gt;= 0.90, 
OR ( ISBLANK(Account.A2Z_CoID__c),
      ISNULL(Account.A2Z_CoID__c) 
   ),

OR (
     ISBLANK(Account.FirstName__c ),
     ISBLANK(Account.LastName__c ),   
     ISBLANK(Account.Email_Main__c ), 
     ISBLANK(Account.BillingStreet), 
     ISBLANK(Account.BillingCity), 
     ISBLANK(Account.BillingCountry), 
     ISBLANK(Account.Phone),
     AND( 
         OR(
          ISPICKVAL(Account.BillingCountryCode,'US'), 
          ISPICKVAL(Account.BillingCountryCode,'CA') 
          ),

        OR( 
           ISBLANK(Account.BillingState), 
           ISBLANK(Account.BillingPostalCode)
          )
        )
     )
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False 
&amp;&amp;  EE_Record_Type__c == "Exhibitor"</errorConditionFormula>
        <errorMessage>The Account for this Opportunity does not have complete information. In order to set the Opportunity to 90% or greater, you must first complete the following fields: [A2Z Contact First &amp; Last Name,A2Z Company E-Mail, HQ Address,Phone] on the Account tab.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
ISBLANK(Amount),
Probability &gt;= 0.5 ,
NOT(ISPICKVAL(Product__c,'TR Package'))
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>Please enter Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_NOT_Allowed_For_TR</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
ABS(Amount) &gt; 0.00,
ISPICKVAL(Product__c,'TR Package')
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>For TR Package, please forecast each product individually.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Booth_Expired</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISPICKVAL(StageName,"Active"),
     ISPICKVAL( Product__c, "Booth"),
     Event_Booth_Last_Date__c &lt; Today())&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Event__c</errorDisplayField>
        <errorMessage>You cannot create a Booth Opportunity for the  [Event] because it is past the date for Opportunity creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Forecast_NFS_Required</fullName>
        <active>true</active>
        <description>changed on 1/22 to accommodate Marketing Operations Stage Change initiative</description>
        <errorConditionFormula>AND
(Probability &gt;= 0.5,
(ISPICKVAL( Product__c, "Booth")),
ISBLANK( Forecast_NSF__c )
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Forecast_NSF__c</errorDisplayField>
        <errorMessage>Forecast NSF is required when forecasting booth revenue</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Booth_expired</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( ISPICKVAL(StageName,"Active"),
 NOT(ISPICKVAL( Product__c, "Booth")), 
 Event_Non_Booth_Last_Date__c &lt; Today())&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Event__c</errorDisplayField>
        <errorMessage>You cannot create a Non-Booth Opportunity for the [Event] because it is past the date for Opportunity creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_TR_With_PackageForecast</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Product__c,'TR Package')),
    Not(ISPICKVAL(StageName,'No Contact Attempted')), 
( Sum_of_Forecast_Amount__c  &gt; 0))&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False
&amp;&amp;  EE_Record_Type__c = "Exhibitor"</errorConditionFormula>
        <errorDisplayField>Product__c</errorDisplayField>
        <errorMessage>Non TR opportunity CANNOT have forecast at package level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Object_code_for_Lost_and_cancelled</fullName>
        <active>true</active>
        <description>updated 4/16
Does not let a user enter an objection code if the stagename is not lost or cancelled</description>
        <errorConditionFormula>AND(
and(
   NOT(ISPICKVAL( StageName,'Lost')),NOT(ISPICKVAL( StageName,'Cancelled'))), NOT(ISBLANK( TEXT(Objection_Code__c ))))
&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Objection_Code__c</errorDisplayField>
        <errorMessage>You cannot enter an OBJECTION CODE  if the Stage is NOT Lost or Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ObjectionCodeonlyforLostandCancelled</fullName>
        <active>true</active>
        <description>Set Objection code only when Opportunity Stage is Lost updated 4/16 OR cancelled</description>
        <errorConditionFormula>AND(
OR(
  ISPICKVAL(StageName,'Lost'),ISPICKVAL(StageName,'Cancelled')),(ISBLANK(Text( Objection_Code__c ))))&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Objection_Code__c</errorDisplayField>
        <errorMessage>An Objection Code is required when the Opportunity is set to Lost or Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_for_NSF</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISPICKVAL( Product__c ,"Booth"))  &amp;&amp; NOT(ISBLANK(  Forecast_NSF__c  ))&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Forecast_NSF__c</errorDisplayField>
        <errorMessage>NSF cannot be entered for Non-Booth opportunities. Please check Product Category</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Related_Opportunity_Event</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
    Related_Opportunity__r.Event__c &lt;&gt; NULL, 
    Event__r.Name &lt;&gt; Related_Opportunity__r.Event__r.Name  
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Related_Opportunity__c</errorDisplayField>
        <errorMessage>You can only relate to an opportunity of the same Event</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubExpo_Is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK( SubExpo__c ),
 Probability &gt; 0.49,
CONTAINS('ASD,NY NOW,IGES,Surf Expo January,Surf Expo September', Show__c ),
NOT (CONTAINS(Event_Name__c, "Demo Day"))
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False &amp;&amp;  EE_Record_Type__c ='Exhibitor'</errorConditionFormula>
        <errorDisplayField>SubExpo__c</errorDisplayField>
        <errorMessage>SubExpo is required when Probability is 50% or greater.</errorMessage>
    </validationRules>
