    <fields>
        <fullName>Event_Code__c</fullName>
        <externalId>false</externalId>
        <formula>if(begins(Subject,"P"), 
MID( 
Subject, 
FIND("|",Subject ) + 1, 
FIND("|",RIGHT(Subject, LEN(Subject) - FIND ("|",Subject ) -1 ))) 
,"")</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Event Code</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
