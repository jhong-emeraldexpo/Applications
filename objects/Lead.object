    <validationRules>
        <fullName>Booth_Expired</fullName>
        <active>false</active>
        <errorConditionFormula>Event_Booth_Last_Date__c &lt; Today()</errorConditionFormula>
        <errorDisplayField>LeadToEvent__c</errorDisplayField>
        <errorMessage>You cannot create a Lead for the {Event] event because it is past the Event Close Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>City_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
(NOT ISBLANK(Street)), 

ISBLANK(City) 
)</errorConditionFormula>
        <errorDisplayField>City</errorDisplayField>
        <errorMessage>Please enter city name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Country_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
OR( 
(NOT ISBLANK(Street)), 
(NOT ISBLANK(City)), 
(NOT ISBLANK(PostalCode)) 

), 
ISBLANK(TEXT(CountryCode)))</errorConditionFormula>
        <errorDisplayField>Country</errorDisplayField>
        <errorMessage>Please select a country.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FirstName_EmailorPhone_Target_Event</fullName>
        <active>true</active>
        <description>FirstName,Email/Phone,Target_Event fields are Required</description>
        <errorConditionFormula>OR( 
AND(ISBLANK(pi_ee_Event_Code__c), 
ISBLANK(Event__c)), 
AND(ISBLANK(Phone), 
ISBLANK(Email)), 
ISBLANK(FirstName))&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False
&amp;&amp; EE_Record_Type__c == "Exhibitor"</errorConditionFormula>
        <errorMessage>First Name,Email/Phone,Targeted Event fields are Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Booth_expired</fullName>
        <active>false</active>
        <errorConditionFormula>Event_Non_Booth_Last_Date__c &lt; Today()</errorConditionFormula>
        <errorDisplayField>LeadToEvent__c</errorDisplayField>
        <errorMessage>You cannot create a Lead for the {Event] event because it is past the Event Close Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Postal_Code_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 

OR( 
ISPICKVAL(CountryCode,'US'), 
ISPICKVAL(CountryCode,'CA') 
), 
(NOT ISBLANK(Street)), 
(NOT ISBLANK(City)), 
ISBLANK(PostalCode) 
)</errorConditionFormula>
        <errorDisplayField>PostalCode</errorDisplayField>
        <errorMessage>Postal Code is required for US or Canadian address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Disposition_If_Unqualified</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
ISPICKVAL( Status, 'Unqualified'),
 ISblank( text(Disposition__c) ) 
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Disposition__c</errorDisplayField>
        <errorMessage>Must select a reason to disqualify a lead</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Other_Disposition</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
   ISPICKVAL(Disposition__c,'Other'),
ISBLANK( Other_Disposition__c ) 
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorDisplayField>Other_Disposition__c</errorDisplayField>
        <errorMessage>Additional info is required if Unqualified Reason is "Other".</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_fields_for_converting_a_lead</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
IsConverted, 
OR( 
 
ISBLANK(FirstName), 
ISBLANK(Event__c), 
AND(ISBLANK(Phone), 
ISBLANK(Email)), 
ISPICKVAL(LeadSource, ""), 
ISPICKVAL(Product__c, "") 
) 
)&amp;&amp; $Permission.Bypass_Validation_rules_for_Users = False</errorConditionFormula>
        <errorMessage>• Fields( First Name, Phone or Email, Target Event, Product, Lead Source ) are required before converting a lead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>State_required_for_specific_Countries</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
OR( 
ISPICKVAL(CountryCode,'US'), 
ISPICKVAL(CountryCode,'CA') 
), 
(NOT ISBLANK(Street)), 
(NOT ISBLANK(City)), 
ISBLANK(TEXT(StateCode)) 
)</errorConditionFormula>
        <errorDisplayField>State</errorDisplayField>
        <errorMessage>State/Province is required for US or Canadian address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Street_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
(NOT ISBLANK(City)), 

ISBLANK(Street) 
)</errorConditionFormula>
        <errorDisplayField>Street</errorDisplayField>
        <errorMessage>Please enter street address.</errorMessage>
    </validationRules>
